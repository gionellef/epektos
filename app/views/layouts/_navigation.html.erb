<% if patient_signed_in? %>
  <li><%= link_to 'Edit account', edit_patient_registration_path %></li>
  <li><%= link_to 'Sign out', destroy_patient_session_path, :method=>'delete' %></li>
<% else %>
  <li><%= link_to 'Sign in', new_patient_session_path %></li>
  <li><%= link_to 'Sign up', new_patient_registration_path %></li>
<% end %>